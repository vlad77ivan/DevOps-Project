<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">
   <mirrors>
       <mirror>
         <id>maven-group</id>
         <mirrorOf>*</mirrorOf>
         <url>http://your-maven/repository/maven-public/</url>
       </mirror>
       <mirror>
         <id>maven-snapshots</id>
         <mirrorOf>*</mirrorOf>
         <url>http://your-maven/repository/maven-snapshots/</url>
       </mirror>
       <mirror>
         <id>maven-releases</id>
         <mirrorOf>*</mirrorOf>
         <url>http://your-maven/repository/maven-releases/</url>
       </mirror>
   </mirrors>
   <servers>
     <server>
       <id>maven-group</id>
       <username>your-user</username>
       <password>your-password</password>
     </server>
     <server>
       <id>maven-snapshots</id>
       <username>your-user</username>
       <password>your-password</password>
     </server>
     <server>
       <id>maven-releases</id>
       <username>your-user</username>
       <password>your-password</password>
     </server>
  </servers>
</settings>
